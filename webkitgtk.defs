;; -*- scheme -*-
; object definitions ...
(define-object Page
  (in-module "GtkWebKit")
  (parent "GtkContainer")
  (c-name "WebKitGtkPage")
  (gtype-id "WEBKIT_GTK_TYPE_PAGE")
)

